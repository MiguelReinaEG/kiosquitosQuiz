{
  "name": "anefito",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:staged": "yarn test:ci",
    "test:ci": "cross-env CI=true jest --watchAll=false --runInBand",
    "check-types": "tsc",
    "lint:staged": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint": "yarn lint:staged .",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|css|scss|html)\"",
    "format": "yarn prettier --write",
    "check-format": "yarn prettier --list-different",
    "prepare": "husky install",
    "preCommit": "yarn check-types && yarn lint",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/native": "^6.0.2",
    "@react-spring/native": "^9.7.3",
    "@shopify/react-native-skia": "0.1.221",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.39.7",
    "@tanstack/react-query": "^5.24.1",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^12.4.3",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.4.1",
    "@types/react": "~18.2.45",
    "@types/react-native-base64": "^0.2.0",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "expo": "~50.0.19",
    "expo-auth-session": "^5.4.0",
    "expo-font": "~11.10.3",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.10",
    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "expo-web-browser": "~12.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6",
    "tailwindcss": "^1",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@types/react": "~18.2.45",
    "babel-plugin-module-resolver": "^5.0.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.0.4",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-universe": "^8.0.0",
    "eslint-plugin-flowtype": "^8.0.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "jest": "^29.2.1",
    "jest-expo": "~50.0.2",
    "lint-staged": "^12.3.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "private": true
}
